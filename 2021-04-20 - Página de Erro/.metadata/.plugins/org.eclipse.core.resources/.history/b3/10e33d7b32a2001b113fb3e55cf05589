package suporte;

public class Unidade {

	String html;
	
	public Unidade(){
		html = "<select name=\"unidade\">";
		html += "<option value=\"Pacote\">Pacote</option>";
		html += "<option value=\"Quilo\">Quilo</option>";
		html += "<option value=\"Unidade\">Unidade</option>";
		html += "</select->";		
	}
	
	public String getUnidade(){
		return html;
	}
	
}
